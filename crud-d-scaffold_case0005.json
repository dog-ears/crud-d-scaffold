/* for Many To Many Relationship with laravel Auth  */

{
    "app_type": "web",                          /* [default:web]  */
    "use_laravel_auth": "true",                  /* [default:false]  */
    "models": [
        {
            "name": "user",              /* [required] format[nameName] */
            "display_name": "USER",     /* [required] */
            "use_soft_delete": "true",          /* [default:false] ex( true, false ) */
            "schemas": [
                {
                    "name": "age",
                    "type": "integer",
                    "input_type": "text",
                    "faker_type": "randomNumber(2)",
                    "nullable": "false",
                    "display_name": "AGE",
                    "show_in_list": "true",
                    "show_in_detail": "true",
					"belongsto": "",
					"belongsto_column": ""
                },{
                    "name": "vitality",
                    "type": "integer",          /* related column froce type to unsigned integer */
                    "input_type": "text",       /* related column froce input_type to pulldown */
                    "faker_type": "numberBetween(1,100)",
                    "nullable": "false",
                    "display_name": "VITALITY",
                    "show_in_list": "true",
                    "show_in_detail": "true",
					"belongsto": "",
					"belongsto_column": ""
                }
            ]               /* Don't write property [ name', 'email', 'password ] In model laravel Auth "user"  */
        },{
            "name": "nicePost",
            "display_name": "NICE POST",
            "use_soft_delete": "true",
            "schemas": [
                {
                    "name": "bigTitle",         /* [required] */
                    "type": "string",           /* [default:string] ex( integer, string ) */
                    "input_type": "text",       /* [default:text] text, textarea */ /* relation_column forced to pulldown */
                    "faker_type": "name()",     /* [default -> ''(nullable must be true) ] ex( randomDigit(), randomNumber(2), numberBetween(1,30), word(), sentence(), paragraph(), text(), name(), address(), date('Y-m-d','now'), safeEmail(), password() )  */
                    "nullable": "false",        /* [default:false] ex( true, false ) */
                    "display_name": "BIG TITLE",/* [required] */
                    "show_in_list": "true",     /* [default:true] ex( true, false ) */
                    "show_in_detail": "true",   /* [default:true] ex( true, false ) */
					"belongsto": "",            /* [default:""] */
					"belongsto_column": ""      /* [default:""] */
                },{
                    "name": "shallowBody",
                    "type": "string",
                    "input_type": "text",
                    "faker_type": "paragraph()",
                    "nullable": "false",
                    "display_name": "SHALLOW BODY",
                    "show_in_list": "true",
                    "show_in_detail": "false",
					"belongsto": "",
					"belongsto_column": ""
                },{
                    "name": "userId",
                    "type": "integer",          /* related column froce type to unsigned integer */
                    "input_type": "text",       /* related column froce input_type to pulldown */
                    "faker_type": "numberBetween(1,30)",
                    "nullable": "false",
                    "display_name": "USER - NAME",
                    "show_in_list": "true",
                    "show_in_detail": "true",
					"belongsto": "user",
					"belongsto_column": "name"
                }
            ]
        },{
            "name": "specialLicense",
            "display_name": "SPECIAL LICENSE",
            "use_soft_delete": "true",
            "schemas": [
                {
                    "name": "licenseName",
                    "type": "string",
                    "input_type": "text",
                    "faker_type": "name()",
                    "nullable": "false",
                    "display_name": "LICENSE NAME",
                    "show_in_list": "true",
                    "show_in_detail": "true",
					"belongsto": "",
					"belongsto_column": ""
                }
            ]
        },{
            "name": "powerfulAbility",
            "display_name": "POWERFUL ABILITY",
            "use_soft_delete": "true",
            "schemas": [
                {
                    "name": "abilityName",
                    "type": "string",
                    "input_type": "text",
                    "faker_type": "name()",
                    "nullable": "false",
                    "display_name": "ABILITY NAME",
                    "show_in_list": "true",
                    "show_in_detail": "true",
					"belongsto": "",
					"belongsto_column": ""
                }
            ]
        },{
            "name": "goodComment",
            "display_name": "GOOD COMMENT",
            "use_soft_delete": "true",
            "schemas": [
                {
                    "name": "commenterName",
                    "type": "string",
                    "input_type": "text",
                    "faker_type": "name()",
                    "nullable": "false",
                    "display_name": "COMMENTER NAME",
                    "show_in_list": "true",
                    "show_in_detail": "true",
					"belongsto": "",
					"belongsto_column": ""
                },{
                    "name": "nicePostId",
                    "type": "integer",
                    "input_type": "text",
                    "faker_type": "numberBetween(1,30)",
                    "nullable": "false",
                    "display_name": "NICE POST - BIG TITLE",
                    "show_in_list": "true",
                    "show_in_detail": "true",
					"belongsto": "nicePost",
					"belongsto_column": "bigTitle"
                }
            ]
        }
    ],
    "pivots":[
        {
            "parentModel":"user",
            "parentModel_column":"name",
            "childModel":"specialLicense",
            "childModel_column":"licenseName",
            "use_soft_delete": "false",
            "schemas": [
                {
                    "name":"proficiency",
                    "type":"integer",
                    "input_type": "text",
                    "faker_type": "numberBetween(1,10)",
                    "nullable": "true",
                    "display_name": "PROFICIENCY"
                },{
                    "name":"rearity",
                    "type":"string",
                    "input_type": "text",
                    "faker_type": "name()",
                    "nullable": "true",
                    "display_name": "REARITY"
                }
            ]
        },{
            "parentModel":"user",
            "parentModel_column":"name",
            "childModel":"powerfulAbility",
            "childModel_column":"abilityName",
            "use_soft_delete": "false",
            "schemas": []
        }
    ]
}