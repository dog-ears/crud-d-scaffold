{
  "name": "dog-ears/crud-d-scaffold",
  "description": "Extend Laravel 7's generators scaffold.",
  "keywords": ["laravel", "generators", "scaffold" , "laravel7" , "CRUD" , "crud"],
  "license": "MIT",
  "minimum-stability": "dev",
  "homepage": "http://dog-ears.net/",
  "authors": [
    {
      "name": "dog-ears",
      "email": "info@dog-ears.com"
    }
  ],
  "require": {
    "laravel/framework": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "DogEars\\CrudDScaffold\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
        "Tests\\Unit\\": "tests/unit/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "DogEars\\CrudDScaffold\\GeneratorsServiceProvider"
      ]
    }
  }
}